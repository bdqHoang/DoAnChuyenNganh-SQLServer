    <script>
        $(document).ready(function () {
            $.ajax({
                type: "Get",
                url: "/home/showcategorize",
                dataType: "json",
                contentType: "application/json",
                success: function (data) {
                    var str = "";
                    for (var i = 0; i < data.length; i++) {
                        str += `<li class="has-dropdown" id="${data[i].CategorizeID}">
                                        <a href="/home/shop">
                                            <i class="fal fa-headphones"></i>
                                                ${data[i].DisplayName}
                                        </a>
                                        <ul class="mega-menu">
                                            <li><a href="shop.html">Trademark</a>
                                                <ul class="mega-item">
                                                    ${GetSupplier(data[i].CategorizeID)}
                                                </ul>
                                            </li>
                                            <li><a href="shop.html">Sell Price</a>
                                                <ul class="mega-item">
                                                    <li><a href="product-details.html">Less than 20 million</a></li>
                                                    <li><a href="product-details.html">From 20 to 25 million VND</a></li>
                                                    <li><a href="product-details.html">From 25 to 30 million VND</a></li>
                                                    <li><a href="product-details.html">Over 30 million</a></li>
                                                </ul>
                                            </li>
                                            <li><a href="shop.html">Sort Product</a>
                                                <ul class="mega-item">
                                                    <li><a href="product-details.html">Top selling</a></li>
                                                    <li><a href="product-details.html">High to low price</a></li>
                                                    <li><a href="product-details.html">Low to high price</a></li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </li>`;
                    }
                    $("#menu-categorize").html(str);
                }
            });
            TopCategorize();
        })
        function GetSupplier(id) {
            var str = '';
            $.ajax({
                async: false,
                type: "Get",
                url: "/home/getsupplier",
                dataType: "json",
                contentType: "application/json",
                data: { id: id },
                success: function (data) {
                    if (data.length != 0) {
                        for (var i = 0; i < data.length; i++) {
                            var name = data[i].DisplayName;
                            str += `<li><a href="/home/shop">${name}</a></li>`;
                        }
                    }
                },
            });
            return str;
        }
    </script>
